{
  "name": "naico-mini-program",
  "version": "1.0.0",
  "private": true,
  "description": "https://naico.wang",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Less",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
